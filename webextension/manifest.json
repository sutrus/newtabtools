{
	"name": "__MSG_extensionName__",
	"description": "__MSG_extensionDescription__",
	"version": "85",
	"icons": {
		"64": "images/icon.svg"
	},
	"applications": {
		"gecko": {
			"id": "newtabtools@darktrojan.net",
			"strict_min_version": "55.0a1"
		}
	},
	"chrome_url_overrides": {
		"newtab": "newTab.xhtml"
	},
	"background": {
		"scripts": [
			"tiles.js",
			"prefs.js",
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": "images/tools-light.svg",
		"default_popup": "action.html",
		"default_title": "__MSG_extensionName__",
		"browser_style": true
	},
	"permissions": [
		"<all_urls>",
		"bookmarks",
		"history",
		"idle",
		"sessions",
		"storage",
		"tabs",
		"topSites",
		"webNavigation"
	],
	"default_locale": "en",
	"manifest_version": 2
}
